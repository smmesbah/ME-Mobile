{
  "expo": {
    "name": "ME",
    "slug": "ME",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dummy_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/dummy_splash_screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.me.ME",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Microphone access permission needed"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.me.ME"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar."
        }
      ],
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Allow ME to access the microphone",
          "speechRecognitionPermission": "Allow ME to securely recognize user speech"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1b9d10fd-0842-4002-be12-744a4ec432df"
      }
    }
  }
}
